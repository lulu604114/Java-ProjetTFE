            <property name="eclipselink.ddl-generation.output-mode" value="both"/>
        <property name="eclipselink.jdbc.url" value="jdbc:mysql://jsf_tfe.must.be:3307/jsf_tfe?autoReconnect=true"/>
        <property name="eclipselink.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      </properties>
    </persistence-unit>
</persistence>